    "@sveltejs/adapter-netlify": "^1.0.0-next.51",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.1.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
